// Place your settings in this file to overwrite default and user settings.
{
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        ".vscode": true,
        "dist": true,
        "node_modules": true,
        "npm-debug.log": true,
        "src/example/*.hash": true
    },
    "javascript.validate.lint.comparisonOperatorsNotStrict": "error",
    "javascript.validate.lint.curlyBracketsMustNotBeOmitted": "warning",
    "javascript.validate.lint.functionsInsideLoops": "warning",
    "javascript.validate.lint.missingSemicolon": "warning",
    "javascript.validate.lint.unusedVariables": "warning"
}